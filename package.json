{
  "name": "panscan",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "type-check": "turbo type-check",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:patient": "turbo dev --filter=@panscan/patient",
    "dev:admin": "turbo dev --filter=@panscan/admin"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@10.9.2"
}
